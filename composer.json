{
    "name": "patchlevel/enum",
    "type": "library",
    "license": "MIT",
    "description": "Small lightweight library to create enum in PHP without SPLEnum",
    "keywords": [
        "enum"
    ],
    "homepage": "https://github.com/patchlevel/enum",
    "authors": [
        {
            "name": "Daniel Badura",
            "email": "d.m.badura@gmail.com"
        },
        {
            "name": "David Badura",
            "email": "d.a.badura@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16.7",
        "infection/infection": "^0.20.1",
        "phpstan/phpstan": "^0.12.56",
        "phpunit/phpunit": "^9.4.3",
        "symfony/var-dumper": "^5.1",
        "vimeo/psalm": "^4.1.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Patchlevel\\Enum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Patchlevel\\Enum\\Tests\\": "tests/",
            "Patchlevel\\Enum\\Example\\": "example/"
        }
    }
}
